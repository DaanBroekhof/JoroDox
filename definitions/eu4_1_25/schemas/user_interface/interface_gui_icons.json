{
  "$id": "http://jorodox.org/schemas/interface_gui_icons.json",
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "nr": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "data": {
      "$ref": "interface_gui_icons.json#/definitions/icon_type"
    }
  },
  "required": [
    "id",
    "data"
  ],
  "definitions": {
    "icon_type": {
      "$mergeProps": [
        {
          "$ref": "interface_gui_basetypes.json#/definitions/base_type"
        },
        {
          "properties": {
            "quadTextureSprite": {
              "$ref": "identifiers.json#/definitions/interface_gui_spritetypes_not_empty"
            },
            "buttonMesh": {
              "$ref": "identifiers.json#/definitions/interface_gui_spritetypes_not_empty"
            },
            "spriteType": {
              "$ref": "identifiers.json#/definitions/interface_gui_spritetypes"
            },
            "position": {
              "$ref": "special_values.json#/definitions/coordinate_xy"
            },
            "frame": {
              "type": "number"
            },
            "scale": {
              "type": "number"
            },
            "rotation": {
              "type": "number"
            },
            "Orientation": {
              "$ref": "special_values.json#/definitions/gui_orientation"
            },
            "orientation": {
              "$ref": "special_values.json#/definitions/gui_orientation"
            },
            "allwaystransparent": {
              "$ref": "special_values.json#/definitions/boolean"
            },
            "alwaystransparent": {
              "todo": "all or al??",
              "$ref": "special_values.json#/definitions/boolean"
            },
            "tooltip": {
              "type": "string",
              "todo": "These seem just to be empty, always... ?",
              "enum": [""]
            },
            "tooltipText": {
              "type": "string",
              "todo": "These seem just to be empty, always... ?",
              "enum": [""]
            },
            "delayedTooltipText": {
              "type": "string",
              "todo": "These seem just to be empty, always... ?",
              "enum": [""]
            },
            "hint_tag": {
              "$ref": "identifiers.json#/definitions/hints"
            },
            "if_resolution": {
              "$allowMultiple": {
                "$mergeProps": [
                  {
                    "properties": {
                      "max_height": {
                        "type": "number"
                      },
                      "min_height": {
                        "type": "number"
                      }
                    }
                  },
                  {
                    "$ref": "interface_gui_icons.json#/definitions/icon_type"
                  }
                ]
              }
            }
          }
        }
      ]
    }
  }
}