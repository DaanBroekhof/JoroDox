        "max_provinces": {
          "type": "number"
        },
        "min_countries": {
          "type": "number"
        },
        "max_countries": {
          "type": "number"
        },
        "min_country_size": {
          "type": "number"
        },
        "max_country_size": {
          "type": "number"
        },
        "min_native_size": {
          "type": "number"
        },
        "max_native_size": {
          "type": "number"
        },
        "min_native_hostility": {
          "type": "number"
        },
        "max_native_hostility": {
          "type": "number"
        },
        "min_native_ferocity": {
          "type": "number"
        },
        "max_native_ferocity": {
          "type": "number"
        },
        "religion": {
          "$ref": "identifiers.json#/definitions/religions"
        },
        "country_religion": {
          "$ref": "identifiers.json#/definitions/religions"
        },
        "technology_group": {
          "$ref": "identifiers.json#/definitions/technology_groups"
        },
        "government": {
          "$ref": "identifiers.json#/definitions/governments"
        },
        "graphical_culture": {
          "$ref": "identifiers.json#/definitions/graphicalculturetypes"
        },
        "culture": {
          "$ref": "identifiers.json#/definitions/cultures"
        },
        "culture_group": {
          "$ref": "identifiers.json#/definitions/culture_groups"
        },
        "unit_type": {
          "$ref": "identifiers.json#/definitions/technology_groups"
        },
        "unique": {
          "$ref": "special_values.json#/definitions/boolean"
        },
        "names": {
          "type": "array",
