        "minor_tags": {
          "$ref": "special_values.json#/definitions/boolean"
        },
        "force_apart": {
          "$ref": "special_values.json#/definitions/boolean"
        },
        "force_coastal": {
          "$ref": "special_values.json#/definitions/boolean"
        },
        "force_together": {
          "$ref": "special_values.json#/definitions/boolean"
        },
        "force_all_provinces_owned": {
