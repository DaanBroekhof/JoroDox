  "type": "object",
  "additionalProperties": false,
  "properties": {
    "nr": {
      "type": "string"
    },
    "comments": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "enum": ["lake"]
    },
    "data": {
      "type": "object",
      "additionalProperties": false,
