{
  "$id": "http://jorodox.org/schemas/interface_gui_curvegraphs.json",
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "nr": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "data": {
      "$ref": "interface_gui_curvegraphs.json#/definitions/curvegraph_type"
    }
  },
  "required": [
    "id",
    "data"
  ],
  "definitions": {
    "curvegraph_type": {
      "$mergeProps": [
        {
          "$ref": "interface_gui_basetypes.json#/definitions/base_type"
        },
        {
          "properties": {
            "position": {
              "$ref": "special_values.json#/definitions/coordinate_xy"
            },
            "size": {
              "$ref": "special_values.json#/definitions/coordinate_xy"
            },
            "spriteType": {
              "$ref": "identifiers.json#/definitions/interface_gui_spritetypes"
            }
          }
        }
      ]
    }
  }
}